@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dash: <http://datashapes.org/dash#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix ex: <http://example.org/> .

ex:iso8601_Shape a sh:NodeShape;
  sh:targetClass ex:iso8601;
  sh:property [
    sh:path ex:iso8601;
    sh:datatype xsd:string;
    sh:description "e.g. 2014-06-29";
    sh:pattern "^([1-2][0-9]{3}-[0-1][0-9]-[0-3][0-9]|[1-2][0-9]{3}-[0-1][0-9]|[1-2][0-9]{3})$";
  ].

ex:JS_id_Shape a sh:NodeShape;
  sh:targetClass ex:JS_id;
  sh:property [
    sh:path ex:$schema;
    sh:datatype xsd:string;
    sh:description "link to the version of the schema that can validate the resume";
  ];
  sh:property [
    sh:path ex:basics;
    sh:node ex:basics_Shape;
  ];
  sh:property [
    sh:path ex:work;
    sh:node dash:ListShape;
    sh:property [
      sh:path ([sh:zeroOrMorePath rdf:rest] rdf:first);
      sh:datatype ex:obj1_Shape;
    ];
  ];
  sh:property [
    sh:path ex:volunteer;
    sh:node dash:ListShape;
    sh:property [
      sh:path ([sh:zeroOrMorePath rdf:rest] rdf:first);
      sh:datatype ex:obj2_Shape;
    ];
  ];
  sh:property [
    sh:path ex:education;
    sh:node dash:ListShape;
    sh:property [
      sh:path ([sh:zeroOrMorePath rdf:rest] rdf:first);
      sh:datatype ex:obj3_Shape;
    ];
  ];
  sh:property [
    sh:path ex:awards;
    sh:node dash:ListShape;
    sh:property [
      sh:path ([sh:zeroOrMorePath rdf:rest] rdf:first);
      sh:datatype ex:obj4_Shape;
    ];
  ];
  sh:property [
    sh:path ex:publications;
    sh:node dash:ListShape;
    sh:property [
      sh:path ([sh:zeroOrMorePath rdf:rest] rdf:first);
      sh:datatype ex:obj5_Shape;
    ];
  ];
  sh:property [
    sh:path ex:skills;
    sh:node dash:ListShape;
    sh:property [
      sh:path ([sh:zeroOrMorePath rdf:rest] rdf:first);
      sh:datatype ex:obj6_Shape;
    ];
  ];
  sh:property [
    sh:path ex:languages;
    sh:node dash:ListShape;
    sh:property [
      sh:path ([sh:zeroOrMorePath rdf:rest] rdf:first);
      sh:datatype ex:obj7_Shape;
    ];
  ];
  sh:property [
    sh:path ex:interests;
    sh:node dash:ListShape;
    sh:property [
      sh:path ([sh:zeroOrMorePath rdf:rest] rdf:first);
      sh:datatype ex:obj8_Shape;
    ];
  ];
  sh:property [
    sh:path ex:references;
    sh:node dash:ListShape;
    sh:property [
      sh:path ([sh:zeroOrMorePath rdf:rest] rdf:first);
      sh:datatype ex:obj9_Shape;
    ];
  ];
  sh:property [
    sh:path ex:projects;
    sh:node dash:ListShape;
    sh:property [
      sh:path ([sh:zeroOrMorePath rdf:rest] rdf:first);
      sh:datatype ex:obj10_Shape;
    ];
  ];
  sh:property [
    sh:path ex:meta;
    sh:node ex:meta_Shape;
  ].

ex:basics_Shape a sh:NodeShape;
  sh:targetClass ex:basics;
  sh:property [
    sh:path ex:name;
    sh:datatype xsd:string;
  ];
  sh:property [
    sh:path ex:label;
    sh:datatype xsd:string;
    sh:description "e.g. Web Developer";
  ];
  sh:property [
    sh:path ex:image;
    sh:datatype xsd:string;
    sh:description "URL (as per RFC 3986) to a image in JPEG or PNG format";
  ];
  sh:property [
    sh:path ex:email;
    sh:datatype xsd:string;
    sh:description "e.g. thomas@gmail.com";
  ];
  sh:property [
    sh:path ex:phone;
    sh:datatype xsd:string;
    sh:description "Phone numbers are stored as strings so use any format you like, e.g. 712-117-2923";
  ];
  sh:property [
    sh:path ex:url;
    sh:datatype xsd:string;
    sh:description "URL (as per RFC 3986) to your website, e.g. personal homepage";
  ];
  sh:property [
    sh:path ex:summary;
    sh:datatype xsd:string;
    sh:description "Write a short 2-3 sentence biography about yourself";
  ];
  sh:property [
    sh:path ex:location;
    sh:node ex:location_Shape;
  ];
  sh:property [
    sh:path ex:profiles;
    sh:node dash:ListShape;
    sh:property [
      sh:path ([sh:zeroOrMorePath rdf:rest] rdf:first);
      sh:datatype ex:obj11_Shape;
    ];
  ].


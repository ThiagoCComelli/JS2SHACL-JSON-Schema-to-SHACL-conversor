@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dash: <http://datashapes.org/dash#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix ex: <http://example.org/> .

ex:JS_id_Shape a sh:NodeShape;
  sh:targetClass ex:JS_id;
  sh:property [
    sh:path ex:genericArray;
    sh:node dash:ListShape;
    sh:property [
      sh:path ([sh:zeroOrMorePath rdf:rest] rdf:first);
      sh:datatype ex:obj1_Shape;
    ];
    sh:minCount 1;
  ].

ex:obj1_Shape a sh:NodeShape;
  sh:targetClass ex:obj1;
  sh:or (
    [
      sh:datatype xsd:string;
      sh:name "The first anyOf schema";
      sh:description "An explanation about the purpose of this instance.";
    ]
    [
      sh:datatype xsd:integer;
      sh:name "The second anyOf schema";
      sh:description "An explanation about the purpose of this instance.";
    ]
  ).
